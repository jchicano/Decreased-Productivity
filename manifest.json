{
   "background": {
      "service_worker": "js/background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery.js", "js/keypress-2.1.4.min.js", "js/dp.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "Discreetly surf the web! Control exactly how you want web pages to look like.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "24": "img/icon24.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "Decreased Productivity",
   "options_page": "options.html",
   "action": {
      "default_icon": "img/addressicon/coffee.png",
      "default_title": "Decreased Productivity"
   },
   "permissions": [ "contextMenus", "tabs", "scripting", "storage" ],
   "host_permissions": [ "http://*/*", "https://*/*" ],
   "version": "0.47.0.1"
}
